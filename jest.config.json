{
   "testEnvironment": "node",
   "testTimeout": 30000,
   "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
   "testMatch": ["<rootDir>/tests/**/*.test.js"],
   "coverageDirectory": "coverage",
   "collectCoverageFrom": ["**/*.js", "!node_modules/**", "!coverage/**", "!tests/**", "!scripts/**"]
}
